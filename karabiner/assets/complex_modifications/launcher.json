{
  "title": "Launcher",
  "rules": [
    {
      "description": "Launch Terminal App",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a Terminal ."
            }
          ]
        }
      ]
    }
  ]
}
